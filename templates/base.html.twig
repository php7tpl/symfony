<!doctype html>
{{ style('/bundles/web/vendor/bootstrap/css/bootstrap.min.css') }}
{{ style('/app/style/sticky-footer.css') }}
{{ style('/bundles/web/vendor/fontawesome-free/css/all.min.css') }}
{{ script('/bundles/web/vendor/jquery/jquery.min.js') }}
{{ script('/bundles/web/vendor/bootstrap/js/bootstrap.bundle.min.js') }}
{{ breadcrumbAdd('<i class="fa fa-home" aria-hidden="true"></i>', '/') }}
{% set bodyCode %}
    {% block body %}{% endblock %}
{% endset %}
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    {% include '@Notify/notify.html.twig' %}
    {% include '@Web/style.html.twig' %}
</head>
<body>
{% include 'widget/navbar.html.twig' %}
<div class="container" style="margin-top: 20px;">
    <nav aria-label="breadcrumb">
        {{ breadcrumb() }}
    </nav>
    {{ bodyCode }}
</div>
<footer class="footer">
    <div class="container">
        {% include 'widget/footer.html.twig' %}
    </div>
</footer>
{% include '@Web/script.html.twig' %}
</body>
</html>