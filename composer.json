{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "friendsofsymfony/user-bundle": "^2.1",
        "guzzlehttp/guzzle": "^6.3",
        "php-ai/php-ml": "^0.8.0",
        "php7lab/bundle": "dev-master",
        "php7lab/core": "dev-master",
        "php7lab/eloquent": "dev-master",
        "php7lab/rest": "dev-master",
        "php7lab/sandbox": "dev-master",
        "php7lab/web": "dev-master",
        "phpseclib/phpseclib": "^2.0",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/expression-language": "4.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "4.*",
        "symfony/framework-bundle": "4.*",
        "symfony/intl": "4.*",
        "symfony/maker-bundle": "^1.13",
        "symfony/monolog-bundle": "^3.1",
        "symfony/process": "4.*",
        "symfony/security-bundle": "4.*",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "4.*",
        "symfony/web-link": "4.*"
    },
    "require-dev": {
        "php7lab/dev": "dev-master",
        "php7lab/test": "dev-master",
        "zndoc/rest-api": "dev-master",
        "symfony/debug-pack": "*",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "symfony/web-server-bundle": "4.*"
    },
    "config": {
        "secure-http": false,
        "process-timeout": 1800,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.*"
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "vendor/{$vendor}/{$name}/": [
                "type:bower-asset",
                "type:npm-asset"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "git",
            "url": "https://github.com/php7lab/sandbox.git"
        },
        {
            "type": "git",
            "url": "https://github.com/php7lab/core.git"
        },
        {
            "type": "git",
            "url": "https://github.com/php7lab/web.git"
        },
        {
            "type": "git",
            "url": "https://github.com/php7lab/dev.git"
        },
        {
            "type": "git",
            "url": "https://github.com/php7lab/eloquent.git"
        },
        {
            "type": "git",
            "url": "https://github.com/php7lab/bundle.git"
        },
        {
            "type": "git",
            "url": "https://github.com/php7lab/rest.git"
        },
        {
            "type": "git",
            "url": "https://github.com/php7lab/test.git"
        }
    ]
}
